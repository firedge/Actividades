<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.fdgproject.firedge.deint_205" >

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Formulario"
            android:label="@string/title_activity_formulario"
            android:parentActivityName=".MainActivity" >
>
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.fdgproject.firedge.deint_205.MainActivity" />
        </activity>
        <activity
            android:name=".DetailView"
            android:label="@string/title_activity_detail_view" >
        </activity>
    </application>

</manifest>
